<!-- Page Title -->
<section class="page-title">
    <div class="content" style="background-image: url(assets/images/background/15.png)">
        <div class="auto-container">
            <h1>About us</h1>
        </div>
    </div>
    <ul class="page-breadcrumb">
        <li><a href="/">home</a></li>
        <li>About us</li>
    </ul>
</section>
<!-- End Page Title -->

<!-- Innovation Section Two / Style Two -->
<section class="experiance-section-two style-two">
	<div class="auto-container">
		<div class="row clearfix">
			<!-- Content Column -->
			<div class="content-column col-lg-6 col-md-12 col-sm-12">
				<div class="inner-column">
					<div class="sec-title">
						<div class="title">Pioneering Innovation Since 2010</div>
						<h2>Transforming <span>Industries</span> <br> with Smart Solutions.</h2>
						<div class="text">GNC Solutions has been at the forefront of technological advancements, delivering high-quality telecom and infrastructure solutions. We focus on innovation, sustainability, and customer satisfaction to drive progress in the industry.</div>
					</div>
					<!-- Fact Counter / Style Three -->
					<div class="fact-counter style-three">
						<div class="row clearfix">
							<!-- Column -->
							<div class="column counter-column col-lg-6 col-md-6 col-sm-12">
								<div class="inner wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="1500ms">
									<div class="content">
										<div class="icon flaticon-helmet"></div>
										<div class="count-outer count-box alternate">
											<span class="count-text" data-speed="5000" data-stop="3500">0</span>+
										</div>
										<h4 class="counter-title">Successful Deployments</h4>
									</div>
								</div>
							</div>

							<!-- Column -->
							<div class="column counter-column col-lg-6 col-md-6 col-sm-12">
								<div class="inner wow fadeInLeft" data-wow-delay="600ms" data-wow-duration="1500ms">
									<div class="content">
										<div class="icon flaticon-blueprint"></div>
										<div class="count-outer count-box">
											<span class="count-text" data-speed="3000" data-stop="5">0</span>M
										</div>
										<h4 class="counter-title">Global Users Reached</h4>
									</div>
								</div>
							</div>
						</div>
					</div>

					<!-- Button Box -->
					<div class="button-box">
						<a class="btn-style-three theme-btn" href="/service"><span class="txt">Discover More</span></a>
					</div>
				</div>
			</div>
			
			<!-- Image Column -->
			<div class="image-column col-lg-6 col-md-12 col-sm-12">
				<div class="inner-column">
					<div class="year-box">
						<div class="count-box">
							0<span class="count-text" data-speed="5000" data-stop="14">0</span>
						</div>
						<span class="years">Years</span>
					</div>
					<div class="image">
						<img src="assets/images/resource/about.png" alt="Innovation" />
					</div>
				</div>
			</div>
		</div>
	</div>
</section>
<!-- End Innovation Section Two -->

<!-- Services Section Two -->
<section class="services-section-two">
    <div class="auto-container">
        
        <!-- Sec Title -->
        <div class="sec-title centered">
            <span class="icon flaticon-telephone"></span>
            <h2>What we <span>do</span></h2>
            <div class="text">South-central construction region since 2005</div>
        </div>
        
        <div class="services-carousel owl-carousel owl-theme">
            
            <% services.forEach(service => { %>
            <!-- Service Block Two -->
            <div class="service-block-two">
                <div class="inner-box">
                    <span class="icon ">
						<img src="<%= service.icon %>" alt="<%= service.title %>">
					</span>
                    <h5><a href="<%= service.link %>"><%= service.title %></a></h5>
                    <div class="text"><%= service.description %></div>
                    <a href="<%= service.link %>" class="read-more">Read More <span class="arrow fa fa-caret-right"></span></a>
                </div>
            </div>
            <% }); %>
            
        </div>
    </div>
</section>
<!-- End Services Section Two -->

<%- include('../Home/video') %>

<%- include('client') %>



